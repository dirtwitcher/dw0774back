<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
   <servlet>
  	<servlet-name>ProfileServlet</servlet-name>
  	<display-name>ProfileServlet</display-name>
  	<description></description>
  	<servlet-class>controllers.ProfileServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ZakazServlet</servlet-name>
  	<display-name>ZakazServlet</display-name>
  	<description></description>
  	<servlet-class>controllers.ZakazServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>ProfileServlet</servlet-name>
  	<url-pattern>/Profile</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ZakazServlet</servlet-name>
  	<url-pattern>/Zakaz</url-pattern>
  </servlet-mapping>
  
</web-app>
